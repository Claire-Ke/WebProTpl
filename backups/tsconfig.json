{
    "files": [
    ],
    "exclude": [
        ".git",
        ".idea",
        "assistTools",
        "backups",
        "bats",
        "configures",
        "dist",
        "node_modules",
        "notes",
        "simServer",
        "src/assets",
        "src/pwa4Manifest",
        "src/static",
        "src/styles",
        "src/tplEJS",
        "src/tplHTML",
        "src/wasm",
        "src/workers",
        "webpackRecords",
        "bower_components",
        "jspm_packages"
    ],
    "include": [
        "src/components/**/*.ts",
        "src/components/**/*.tsx",
        "src/js/**/*.ts",
        "src/js/**/*.tsx",
        "src/vue/**/*.ts",
        "src/vue/**/*.tsx",
        "src/vue/**/*.vue",
        "src/webComponents/**/*.ts",
        "src/webComponents/**/*.tsx"
    ],
    "extends": "",
    "typeAcquisition": {
        "enable": true,
        "include": [],
        "exculde": []
    },
    "compileOnSave": true,
    "compilerOptions": {
        "allowJs": false,
        "checkJs": false,
        "alwaysStrict": true,
        "downlevelIteration": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "onlyRemoveTypeImports": true,
        "importsNotUsedAsValues": "preserve",
        "jsx": "preserve",
        "module": "ES2015",
        "sourceMap": false,
        "target": "es5",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "lib": [
            "dom",
            "es2015",
            "es2016",
            "es2017",
            "esnext"
        ],
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "strictNullChecks": false,
        "suppressImplicitAnyIndexErrors": true,
        "types": [
            "jest"
        ]
    }
}
